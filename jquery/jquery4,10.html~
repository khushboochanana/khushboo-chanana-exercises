<html>
<head>
<script src="jquery-1.9.0.js"></script>
<script>
$(document).ready(function(){

	$("#tt").hover(function(){
		$(this).width(100);

		$(this).height(100);
	});
	$("#b1").click(function(){
		$("td:eq(1)").css("color","blue");
	});

	$("#b2").click(function(){
		$("td:lt(1)").css("color","red");
	});

	$("#b3").click(function(){
		$("td:gt(1)").css("color","green");
	});

	$("#b4").click(function(){
		
	        var rows = $("#tt tr");

		rows.eq(0).insertAfter(rows.eq( 1));
	
	});

	$("#b5").click(function(){
		
	         $('#select1 option').eq(2).attr("selected","selected");
	});

	$("#b6").click(function(){
		
	        $('#select1 option' ).filter(':odd').css('color' , 'red');
	});
$("#b7").click(function(){
alert ("hello");
	$("#tt  td").each(function(){

var col=$(this);
if(col.text()>5)
col.css("background","pink");
 
});
});
$("#b8").click(function(){
alert("hiii");
$('div').not('.parent').remove();

});
});
	
</script>
</head>
