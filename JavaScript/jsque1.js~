function si(){

var amount=prompt("enter amount","1000");
if(amount!=null){

	var year=prompt("enter year","2");

	if(year!=null){

		var rate=prompt("enter rate","10");
		if(amount!=null){
			var si=(amount*year*rate)/100;
			document.getElementById("interst").innerHTML=si;
			
			}
		}
	}
}






