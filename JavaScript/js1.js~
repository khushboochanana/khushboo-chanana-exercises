function fun5(){
var d = new Date();
var str= d.getDate()+"  "+d.getMonth()+"  "+ d.getFullYear()+ "  "+d.getHours()+":"+d.getMinutes()+":"+d.getSeconds();
document.getElementById("show").innerHTML =str;
}
