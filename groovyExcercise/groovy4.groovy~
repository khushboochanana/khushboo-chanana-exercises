class Person
{
String name
int empID
String company
float salary


String toString()
    {
       "${name} is a woman aged 24 who lives at Delhi. He works for ${company} with employee id ${empID} and draws ${salary} lots of money !!!!."
    }
}

Person p=new Person(name:"khushboo",empID:15,company:"Intelligrape",salary:10000.00)